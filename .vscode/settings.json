{
  "files.eol": "\n",
  "editor.codeActionsOnSave": {
    "source.fixAll": true,
    "source.fixAll.eslint": false
  },
  "editor.formatOnSave": true,
  "editor.tabSize": 2,
  "javascript.validate.enable": false,
  "editor.rulers": [
    80
  ],
  "cSpell.words": [
    "UNSECURE",
    "brainfuck",
    "camelcase",
    "cloudmersive",
    "codeql",
    "deepai",
    "markdownlint",
    "picpurify",
    "semver",
    "sightengine",
    "simbo",
    "vercel"
  ]
}
