{
  "name": "nsfw-detection-action",
  "version": "1.3.6",
  "description": "This GitHub action detects nsfw content in commited files.",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fabasoad/nsfw-detection-action.git"
  },
  "author": "Yevhen Fabizhevskyi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fabasoad/nsfw-detection-action/issues"
  },
  "keywords": [
    "actions",
    "nsfw",
    "ai"
  ],
  "scripts": {
    "build": "ncc build -m src/index.js",
    "lint": "eslint src __tests__",
    "security:auth": "snyk auth",
    "security:test": "snyk test",
    "test": "nyc mocha --timeout 10000 __tests__",
    "snyk-protect": "snyk protect",
    "prepublish": "yarn run snyk-protect"
  },
  "homepage": "https://github.com/fabasoad/nsfw-detection-action#readme",
  "dependencies": {
    "@actions/core": "1.2.6",
    "@actions/github": "4.0.0",
    "form-data": "3.0.0",
    "got": "11.8.1",
    "snyk": "1.437.3"
  },
  "devDependencies": {
    "@vercel/ncc": "0.26.1",
    "chai": "4.2.0",
    "dotenv": "8.2.0",
    "eslint": "7.16.0",
    "eslint-config-google": "0.14.0",
    "git-branch-is": "4.0.0",
    "husky": "4.3.6",
    "mocha": "8.2.1",
    "mocha-param": "2.0.1",
    "nyc": "15.1.0"
  },
  "snyk": true,
  "husky": {
    "hooks": {
      "pre-commit": "git-branch-is --not main && yarn run build && git add dist/index.js",
      "pre-push": "yarn run lint && yarn test"
    }
  }
}
