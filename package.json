    "@types/jest": "29.5.12",
    "@types/node": "20.12.7",
    "@typescript-eslint/eslint-plugin": "7.6.0",
    "@typescript-eslint/parser": "7.6.0",
    "@vercel/ncc": "0.38.1",
    "eslint": "9.0.0",
    "eslint-config-google": "0.14.0",
    "jest": "29.7.0",
    "jest-circus": "29.7.0",
    "mocha-param": "2.0.1",
