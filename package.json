    "actions",
    "nsfw",
    "ai"
  ],
  "homepage": "https://github.com/fabasoad/nsfw-detection-action#readme",
  "dependencies": {
    "@actions/core": "1.10.0",
    "@actions/github": "5.1.1",
    "form-data": "4.0.0",
    "winston": "3.8.2"
  },
  "devDependencies": {
    "@octokit/types": "8.2.1",
    "@types/chai": "4.3.5",
    "@types/glob": "8.1.0",
    "@types/jest": "29.5.1",
    "@types/node": "18.16.5",
    "@typescript-eslint/eslint-plugin": "5.59.2",
    "@typescript-eslint/parser": "5.59.2",
    "@vercel/ncc": "0.36.1",
    "eslint": "8.40.0",
    "eslint-config-google": "0.14.0",
    "jest": "29.5.0",
    "jest-circus": "29.5.0",
    "mocha-param": "2.0.1",
    "ts-jest": "29.1.0",
    "typescript": "5.0.4"
  },
  "packageManager": "yarn@3.5.1"
