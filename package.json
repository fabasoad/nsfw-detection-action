{
  "name": "nsfw-detection-action",
  "version": "1.3.16",
  "description": "This GitHub action detects nsfw content in committed files.",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fabasoad/nsfw-detection-action.git"
  },
  "author": "Yevhen Fabizhevskyi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fabasoad/nsfw-detection-action/issues"
  },
  "keywords": [
    "actions",
    "nsfw",
    "ai"
  ],
  "scripts": {
    "build": "ncc build -m src/index.ts",
    "lint:md": "markdownlint --config .markdownlint.yml '**/*.md'",
    "lint:es": "eslint --ext ts __tests__",
    "lint": "yarn run lint:md && yarn run lint:es",
    "postinstall": "yarn husky install",
    "prepare": "yarn run snyk-protect",
    "security:auth": "snyk auth",
    "security:test": "snyk test",
    "snyk-protect": "snyk protect",
    "test": "jest --config=jest.config.json --coverage"
  },
  "homepage": "https://github.com/fabasoad/nsfw-detection-action#readme",
  "dependencies": {
    "@actions/core": "1.6.0",
    "@actions/github": "5.0.0",
    "form-data": "4.0.0",
    "got": "12.0.0",
    "snyk": "1.813.0"
  },
  "devDependencies": {
    "@octokit/plugin-enterprise-server": "10.6.4",
    "@types/got": "9.6.12",
    "@types/jest": "27.4.0",
    "@typescript-eslint/eslint-plugin": "5.8.1",
    "@typescript-eslint/parser": "5.8.1",
    "@vercel/ncc": "0.33.1",
    "chai": "4.3.4",
    "dotenv": "10.0.0",
    "eslint": "8.6.0",
    "eslint-config-google": "0.14.0",
    "git-branch-is": "4.0.0",
    "husky": "7.0.4",
    "jest": "27.4.5",
    "jest-circus": "27.4.5",
    "markdownlint-cli": "0.30.0",
    "mocha-param": "2.0.1",
    "ts-jest": "27.1.2",
    "typescript": "4.5.4"
  },
  "snyk": true
}
